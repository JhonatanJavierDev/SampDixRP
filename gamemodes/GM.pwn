		DestroyDynamicObject(PLANTS[info[1]][plant_OBJECT_ID]);
		DestroyDynamic3DTextLabel(PLANTS[info[1]][plant_LABEL_ID]);

		new tmp_PLANTS[PLANTS_ENUM]; PLANTS[info[1]] = tmp_PLANTS;
	}
	return 1;
}

