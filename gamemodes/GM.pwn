								for(new i = 0; i != MAX_PROPIEDADES; i ++)
								{
									if(!PROPERTY_INFO[i][property_VALID]) continue;
									if(PROPERTY_INFO[i][property_OWNER_ID] != PI[playerid][pi_ID]) continue;

									new label_str[256];
									format
									(
										label_str,
										sizeof label_str,
