				}
				else SendClientMessage(playerid, -1, "{"#SILVER_COLOR"}No hay ningún taxista en servicio, llame más tarde.");
			}
			return 1;
		}
		case 570: //Mecánico
		{
			if(PLAYER_TEMP[playerid][pt_WANT_MECANICO])
			{
				PLAYER_TEMP[playerid][pt_WANT_MECANICO] = false;
