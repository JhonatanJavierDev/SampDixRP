				}
				mysql_format(mysql_db, QUERY_BUFFER, sizeof QUERY_BUFFER, "SELECT id FROM player WHERE name = '%e';", name);
				mysql_tquery_inline(mysql_db, QUERY_BUFFER, using inline OnDialogQueryLoad, "s", name);
