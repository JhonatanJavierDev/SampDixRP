GetTimeJail(playerid)
{
	if(PI[playerid][pi_STATE] == ROLEPLAY_STATE_JAIL)
	{
		new time = PI[playerid][pi_POLICE_JAIL_TIME] - (gettime() - PLAYER_TEMP[playerid][pt_ENTER_JAIL_TIME]);
		if(time <= 0)
		{
			UnjailPlayer(playerid);
			return 1;
		}
		return time;
	}
	return -1;
}

