	if(PI[playerid][pi_BANK_ACCOUNT] == 0) return SendNotification(playerid, "No tienes cuenta bancaria, puedes crearla en cualquier Banco.");

	PLAYER_TEMP[playerid][pt_PLAYER_IN_ATM] = true;
